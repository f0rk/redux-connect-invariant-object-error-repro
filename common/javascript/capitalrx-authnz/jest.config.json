{
  "testURL": "http://localhost/",
  "moduleNameMapper": {
    "\\.(pdf|jpg|jpeg|png|gif|ico|xml|manifestjson|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
    "\\.(css|sass)$": "<rootDir>/__mocks__/styleMock.js"
  },
  "transform": {
    "^.+\\.jsx?$": "babel-jest"
  },
  "transformIgnorePatterns": [
    "node_modules/(?!capitalrx-.+)"
  ],
  "globals": {
    "site": {
      "baseUrl": "x.x.x.x:0"
    }
  }
}
